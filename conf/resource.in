{

  "syslog-config" : {
    "patterndb-relpath" : "etc/syslog-ng.conf",
    "logrotate-relpath" : "etc/logrotate.conf"
  },

  "data-share" : {
    "shares" : [
      {
        "name" : "@@SHARE_NAME@@",
        "permission" : {
          "ro" : ["admin"],
          "rw" : ["@@PKG_USER@@"]
        }
      }
    ]
  }

}
